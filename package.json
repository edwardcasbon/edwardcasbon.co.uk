{
  "name": "edwardcasbon.co.uk",
  "version": "1.0.0",
  "description": "Personal website",
  "main": "index.js",
  "scripts": {
    "sass": "node-sass assets/css/styles.scss assets/css/styles.css --output-style compressed --include-path assets/css/partials",
